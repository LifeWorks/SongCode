{
  "name": "songcode",
  "displayName": "Song's VSCode",
  "description": "Personal VSCode extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "songcode.write",
        "title": "Write to File"
      },
      {
        "command": "songcode.quit",
        "title": "Quit Editor"
      }
    ]
  },
  "activationEvents": [
    "onCommand: songcode.write",
    "onCommand: songcode.quit"
  ],
  "main": "./extension.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "vscode-test": "^1.6.2",
    "vscode": "^1.77.0"
  }
}
